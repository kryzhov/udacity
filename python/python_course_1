def which_prize(points):
#The function defines the prize value of the game
    if points <0 or points >200:
        prize = 'Crooked'
    elif points <= 50:
        prize = 'wooden rabbit'
    elif points >= 181:
        prize = 'penguin'
    elif points >= 151:
        prize = 'wafer-thin mint'
    else:
        prize = 'None'

    if prize == "None":
        message='Oh dear, no prize this time.'
    elif prize == "Crooked":
        message="The game is crooked"
    else:
        message="Congratulations! You have won a {}!".format(prize)
    return message

print (which_prize(900))
print (which_prize(-10))
print (which_prize(90))
print (which_prize(190))
